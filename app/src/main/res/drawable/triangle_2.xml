<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate android:fromDegrees="45.5"
            android:pivotX="-10"
            android:pivotY="80">
            <shape
                android:shape="rectangle">
                <size
                    android:width="40dp"
                    android:height="30dp"/>
                <corners
                    android:radius="1dp"/>
                <solid android:color="#f5fcf7"/>
            </shape>
        </rotate>
    </item>
</layer-list>
